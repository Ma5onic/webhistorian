{
  "manifest_version": 2,
  "name": "Web Historian - Web Browsing History Visualizations",
  "version": "0.0.2",
  "short_name": "Web Historian",
  "description": "Web Historian - See your web browsing history in a new light",
  "permissions": [
    "history",
    "identity",
    "tabs"
  ],
  "offline_enabled": true,
  "browser_action": {
    "default_icon": "star48.png"
  },
  "icons": { "16": "star16.png",
    "48": "star48.png",
    "128": "star128.png" },
  "background": {
    "scripts": ["background.js"]
  }
}
