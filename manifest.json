{
  "name": "Web Historian - Web Browsing History Visualizations",
  "version": "0.0.2",
  "short_name": "Web Historian",
  "description": "Web Historian helps you visualize the web browsing history that is already on your computer in a way youâ€™ve never seen before.",
  "permissions": [
    "history",
    "identity",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "browser_action": {
    "default_icon": "her48.png"
  },
  "icons": { "16": "her16.png",
    "48": "her48.png",
    "128": "her128.png" },
