{
  "manifest_version": 2,
  "name": "Web Historian - Web Browsing History Visualizations",
  "version": "0.0.5",
  "short_name": "Web Historian",
  "description": "Web Historian - See your web browsing history in a new light",
  "permissions": [
    "history",
    "identity",
    "identity.email",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "images/owl48.png"
  },
  "icons": { "16": "images/owl16.png",
    "48": "images/owl48.png",
    "128": "images/owl128.png" },
  "background": {
    "scripts": ["js/app/chrome.js"]
  },
  "offline_enabled": true
}
